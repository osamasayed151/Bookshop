<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/a"
    tools:context=".welcome.Welcome">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/margintop_image_welcome"
        android:src="@drawable/person_reading"
        android:contentDescription="@string/todo" />

    <ImageView
        android:layout_width="@dimen/width_image_welcome"
        android:layout_height="@dimen/height_image_welcome"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/margintop_image2_welcome"
        android:scaleType="centerCrop"
        android:src="@drawable/book_shop"
        android:contentDescription="@string/todo" />


</RelativeLayout>